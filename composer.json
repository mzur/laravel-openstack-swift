{
    "name": "mzur/laravel-openstack-swift",
    "description": "OpenStack Swift storage driver for Laravel/Lumen.",
    "type": "library",
    "require": {
        "nimbusoft/flysystem-openstack-swift": "^1.0",
        "biigle/laravel-cached-openstack": "^1.5",
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/filesystem": "^9.0 || ^10.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Zurowietz",
            "email": "martin@zurowietz.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mzur\\Filesystem\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mzur\\Filesystem\\SwiftServiceProvider"
            ]
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.12"
    }
}
